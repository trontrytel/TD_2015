load 'consts.gpi'
load 'lib.gpi'
load 'tests.gpi'

set encoding utf8
set term x11
set xlabel 'temperatura [K]'
set ylabel 'wysokość [m]'
set xrange [120:300]
set yrange [0:16000]

set parametric
set trange[0:16000]

plot \
   ISA_T(t), t                 lw 3 title 'atmosfera standardowa',\
   ISA_0_T - g / c_p_d * t, t  lw 3 title 'gradient suchoadiabatyczny',\
   'pseudoadiabata' using 2:1       title 'gradient wilgotnoadiabatyczny'
